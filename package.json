{
  "name": "tst-reflect-json-schema-generator",
  "version": "1.0.0",
  "description": "Generate JSON schema from tst-reflect types",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "ttsc",
    "build:watch": "nodemon --watch src --ext ts --exec ttsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix --ext .ts .",
    "lint-staged": "lint-staged",
    "pre-commit": "npm run format && npm run lint-staged && npm run build && npm run test",
    "test": "jest test --verbose",
    "test:watch": "jest test --watch --verbose"
  },
  "keywords": [
    "tst-reflect",
    "typescript",
    "json schema"
  ],
  "author": "Joe Ferner <joe@fernsroth.com>",
  "license": "MIT",
  "dependencies": {
    "@types/json-schema": "^7.0.11",
    "nested-error-stacks": "^2.1.1",
    "tst-reflect": "../zzzexternal/tst-reflect/runtime"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/nested-error-stacks": "^2.1.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "tst-reflect-transformer": "../zzzexternal/tst-reflect/transformer",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}